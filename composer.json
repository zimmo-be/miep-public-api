{
  "name": "zimmobe/miep-php-api-client",
  "type": "library",
  "description": "Max-immo External Partners API Client",
  "homepage": "https://github.com/zimmo-be/miep-php-api-client",
  "keywords": ["miep", "partner", "ep", "api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Zimmo.be Team",
      "homepage": "https://www.zimmo.be"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "psr/http-client-implementation": "^1.0",
    "psr/http-factory-implementation": "^1.0",
    "shrikeh/teapot": "^2.3"
  },
  "require-dev": {
    "doctrine/coding-standard": "^9.0",
    "guzzlehttp/guzzle": "^7.2",
    "phpstan/phpstan": "^1.2",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0",
    "vimeo/psalm": "^4.13"
  },
  "autoload": {
    "psr-4": {
      "MaxImmo\\ExternalParties\\": "src/",
      "Tests\\MaxImmo\\ExternalParties\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
